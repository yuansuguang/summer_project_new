\r\n  export default {\r\n    data() {\r\n      return {\r\n        favoriteQuestionnaires: [\r\n          { id: 1, name: '问卷1', publishTime: '2024-05-01', answersCount: 20 },\r\n          { id: 2, name: '问卷2', publishTime: '2024-05-10', answersCount: 15 }\r\n        ]\r\n      };\r\n    },\r\n    methods: {\r\n      clearData(questionnaireId) {\r\n        // 清空问卷数据操作\r\n        console.log('清空问卷数据，问卷ID：', questionnaireId);\r\n      },\r\n      removeFromFavorites(questionnaireId) {\r\n        // 取消收藏操作\r\n        console.log('取消收藏问卷，问卷ID：', questionnaireId);\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* 样式 */\r\n  .white-board {\r\n    background-color: white;\r\n    border: 1px solid black;\r\n    padding: 20px;\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  th, td {\r\n    border: 1px solid black;\r\n    padding: 8px;\r\n    text-align: left;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAkCE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,sBAAsB,EAAE,CACtB;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,KAAK;QAAEC,WAAW,EAAE,YAAY;QAAEC,YAAY,EAAE;MAAG,CAAC,EACnE;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,KAAK;QAAEC,WAAW,EAAE,YAAY;QAAEC,YAAY,EAAE;MAAG;IAEtE,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,eAAe,EAAE;MACzB;MACAC,OAAO,CAACC,GAAG,CAAC,cAAc,EAAEF,eAAe,CAAC;IAC9C,CAAC;IACDG,mBAAmBA,CAACH,eAAe,EAAE;MACnC;MACAC,OAAO,CAACC,GAAG,CAAC,cAAc,EAAEF,eAAe,CAAC;IAC9C;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}