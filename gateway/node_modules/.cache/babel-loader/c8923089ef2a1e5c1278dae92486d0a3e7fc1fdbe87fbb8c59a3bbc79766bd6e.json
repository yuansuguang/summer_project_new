{"ast":null,"code":"export default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      confirmPassword: '',\n      phone: '',\n      verificationCode: '',\n      verificationCodeSent: false\n    };\n  },\n  methods: {\n    register() {\n      if (this.password !== this.confirmPassword) {\n        alert('密码与确认密码不匹配');\n        return;\n      }\n\n      // 在这里执行注册操作，可以发送请求到后端进行用户注册\n      // 此处为示例，仅在控制台输出注册信息\n      console.log('用户名：', this.username);\n      console.log('密码：', this.password);\n      console.log('手机号：', this.phone);\n      console.log('验证码：', this.verificationCode);\n\n      // 注册成功后可以进行页面跳转或其他操作\n    },\n    sendVerificationCode() {\n      // 在这里执行发送验证码操作，可以发送请求到后端发送验证码到用户手机\n      // 此处为示例，仅在控制台输出验证码发送成功信息\n      console.log('发送验证码到手机号：', this.phone);\n      this.verificationCodeSent = true;\n    }\n  }\n};","map":{"version":3,"names":["data","username","password","confirmPassword","phone","verificationCode","verificationCodeSent","methods","register","alert","console","log","sendVerificationCode"],"sources":["E:\\mytest\\src\\views\\accountpane\\registerPane.vue"],"sourcesContent":["<template>\r\n    <div class=\"register-container\">\r\n      <h2>问卷星球注册</h2>\r\n      <form @submit.prevent=\"register\">\r\n        <div class=\"form-group\">\r\n          <label for=\"username\">用户名：</label>\r\n          <input type=\"text\" id=\"username\" v-model=\"username\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"password\">密码：</label>\r\n          <input type=\"password\" id=\"password\" v-model=\"password\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"confirmPassword\">确认密码：</label>\r\n          <input type=\"password\" id=\"confirmPassword\" v-model=\"confirmPassword\" required>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"phone\">手机号：</label>\r\n          <input type=\"tel\" id=\"phone\" v-model=\"phone\">\r\n        </div>\r\n        <div class=\"form-group\" v-if=\"phone\">\r\n          <label for=\"verificationCode\">验证码：</label>\r\n          <input type=\"text\" id=\"verificationCode\" v-model=\"verificationCode\" required>\r\n          <button @click.prevent=\"sendVerificationCode\" :disabled=\"verificationCodeSent\">发送验证码</button>\r\n        </div>\r\n        <button type=\"submit\">注册</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        username: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        phone: '',\r\n        verificationCode: '',\r\n        verificationCodeSent: false\r\n      };\r\n    },\r\n    methods: {\r\n      register() {\r\n        if (this.password !== this.confirmPassword) {\r\n          alert('密码与确认密码不匹配');\r\n          return;\r\n        }\r\n  \r\n        // 在这里执行注册操作，可以发送请求到后端进行用户注册\r\n        // 此处为示例，仅在控制台输出注册信息\r\n        console.log('用户名：', this.username);\r\n        console.log('密码：', this.password);\r\n        console.log('手机号：', this.phone);\r\n        console.log('验证码：', this.verificationCode);\r\n  \r\n        // 注册成功后可以进行页面跳转或其他操作\r\n      },\r\n      sendVerificationCode() {\r\n        // 在这里执行发送验证码操作，可以发送请求到后端发送验证码到用户手机\r\n        // 此处为示例，仅在控制台输出验证码发送成功信息\r\n        console.log('发送验证码到手机号：', this.phone);\r\n        this.verificationCodeSent = true;\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .register-container {\r\n    max-width: 400px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n  }\r\n  \r\n  input[type=\"text\"],\r\n  input[type=\"password\"],\r\n  input[type=\"tel\"] {\r\n    width: 100%;\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n  \r\n  button {\r\n    padding: 10px 20px;\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 3px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:disabled {\r\n    background-color: #ccc;\r\n    cursor: not-allowed;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA+BE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,KAAK,EAAE,EAAE;MACTC,gBAAgB,EAAE,EAAE;MACpBC,oBAAoB,EAAE;IACxB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACN,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;QAC1CM,KAAK,CAAC,YAAY,CAAC;QACnB;MACF;;MAEA;MACA;MACAC,OAAO,CAACC,GAAG,CAAC,MAAM,EAAE,IAAI,CAACV,QAAQ,CAAC;MAClCS,OAAO,CAACC,GAAG,CAAC,KAAK,EAAE,IAAI,CAACT,QAAQ,CAAC;MACjCQ,OAAO,CAACC,GAAG,CAAC,MAAM,EAAE,IAAI,CAACP,KAAK,CAAC;MAC/BM,OAAO,CAACC,GAAG,CAAC,MAAM,EAAE,IAAI,CAACN,gBAAgB,CAAC;;MAE1C;IACF,CAAC;IACDO,oBAAoBA,CAAA,EAAG;MACrB;MACA;MACAF,OAAO,CAACC,GAAG,CAAC,YAAY,EAAE,IAAI,CAACP,KAAK,CAAC;MACrC,IAAI,CAACE,oBAAmB,GAAI,IAAI;IAClC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}