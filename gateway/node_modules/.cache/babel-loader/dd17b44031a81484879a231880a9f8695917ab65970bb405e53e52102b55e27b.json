   <button class=\"button\">星标问卷</button>\r\n        <button class=\"button\">回收站</button>\r\n        <button class=\"button\">文件夹</button>\r\n        <div id=\"description\">\r\n          <div id=\"description-text\">绑定微信后，可在手机同步编辑、管理问卷，实时掌握数据动态。</div>\r\n          <button class=\"button\" id=\"bind-wechat\">绑定微信</button>\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Content -->\r\n      <div id=\"content\">\r\n        <div class=\"toolbar\">\r\n          <div id=\"survey-title\">问卷列表</div>\r\n            <div class=\"sort-container\" @mouseenter=\"showSortOptions = true\" @mouseleave=\"showSortOptions = false\">\r\n              <button>时间排序</button>\r\n                <div v-if=\"showSortOptions\" class=\"sort-options\">\r\n                    <label>\r\n                      <input type=\"checkbox\" v-model=\"ascendingChecked\" @click=\"updateSort\"> 时间正序\r\n                    </label>\r\n                    <label>\r\n                      <input type=\"checkbox\" v-model=\"descendingChecked\" @click=\"updateSort\"> 时间倒序\r\n                    </label>\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"realease-container\" @mouseenter=\"showRealeaseOptions = true\" @mouseleave=\"showRealeaseOptions = false\">\r\n              <button>状态</button>\r\n                <div v-if=\"showRealeaseOptions\" class=\"realease-options\">\r\n                  <label>\r\n                    <input type=\"checkbox\" v-model=\"realeaseChecked\" @click=\"updateRealease\"> 已发布\r\n                  </label>\r\n                  <label>\r\n                    <input type=\"checkbox\" v-model=\"notRealeaseChecked\" @click=\"updateRealease\"> 未发布\r\n                  </label>\r\n                </div>\r\n            </div>\r\n            \r\n            <div id=\"search-box\">\r\n            <input type=\"text\" placeholder=\"请输入问卷名进行搜索\">\r\n            <button @click=\"search\">搜索</button>\r\n            </div>\r\n        </div>\r\n        <div class=\"survey-page\">\r\n          <qnInstance v-for=\"(questionnaire, index) in paginatedQuestionnaires\" :key=\"index\" :is=\"qnInstance\" \r\n               :questionnaireName=\"questionnaire.questionnaireName\" \r\n               :questionnaireId=\"questionnaire.questionnaireId\" \r\n               :isPublished=\"questionnaire.isPublished\" \r\n               :answersCount=\"questionnaire.answersCount\" \r\n               :creationDate=\"questionnaire.creationDate\" />\r\n\r\n          <div class=\"pagination\" v-if=\"totalPages > 1\">\r\n            <button @click=\"previousPage\" :disabled=\"currentPage === 1\">上一页</button>\r\n            <span>第 {{ currentPage }} 页 / 共 {{ totalPages }} 页</span>\r\n            <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\">下一页</button>\r\n          </div>\r\n        </div>\r\n\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n\r\n\r\n  <script>  \r\n  import qnInstance from '@/views/qn-manage/qnInstance'\r\n  export default {\r\n    components: {\r\n      qnInstance,\r\n    },\r\n    data() {\r\n        return {\r\n            showSortOptions: false,\r\n            ascendingChecked: false,\r\n            descendingChecked: false,\r\n            showRealeaseOptions:false,\r\n            realeaseChecked:false,\r\n            notRealeaseChecked:false,\r\n            questionnaires:[],\r\n            itemsPerPage:4,\r\n            currentPage:2,\r\n        };\r\n    },\r\n    created() {\r\n      // 假设从后端获取问卷实例数据的方法为 fetchQuestionnaires()\r\n      this.fetchQuestionnaires();\r\n    },\r\n    computed: {\r\n      paginatedQuestionnaires() {\r\n        const startIndex = (this.currentPage - 1) * this.itemsPerPage;\r\n        return this.questionnaires.slice(startIndex, startIndex + this.itemsPerPage);\r\n      },\r\n      totalPages() {\r\n        return Math.ceil(this.questionnaires.length / this.itemsPerPage);\r\n      },\r\n    },\r\n    methods: {\r\n      search() {\r\n        //这里是添加搜索方法的\r\n        console.log(\"Searching...\");\r\n      },\r\n      updateSort() {\r\n        if(this.ascendingChecked)\r\n        {\r\n          this.ascendingChecked = false;\r\n        }\r\n        else\r\n        {\r\n          this.ascendingChecked = true;\r\n          if(this.descendingChecked)\r\n          {\r\n            this.descendingChecked = false;\r\n          }\r\n        }\r\n      },\r\n      updateRealease() {\r\n        if(this.realeaseChecked)\r\n        {\r\n          this.realeaseChecked = false;\r\n        }\r\n        else\r\n        {\r\n          this.notRealeaseChecked = true;\r\n          if(this.notRealeaseChecked)\r\n          {\r\n            this.notRealeaseChecked = false;\r\n          }\r\n        }\r\n      },\r\n      fetchQuestionnaires() {\r\n      // 假设从后端获取问卷实例数据的过程\r\n        const questionnaireDataFromBackend = [\r\n          {\r\n            questionnaireName: '问卷1',\r\n            questionnaireId: '123',\r\n            isPublished: true,\r\n            answersCount: 50,\r\n            creationDate: '2024-05-20'\r\n          },\r\n          {\r\n            questionnaireName: '问卷2',\r\n            questionnaireId: '456',\r\n            isPublished: false,\r\n            answersCount: 30,\r\n            creationDate: '2024-05-21'\r\n          },\r\n          {\r\n            questionnaireName: '问卷3',\r\n            questionnaireId: '789',\r\n            isPublished: false,\r\n            answersCount: 50,\r\n            creationDate: '2024-04-21'\r\n          },\r\n          {\r\n            questionnaireName: '问卷4',\r\n            questionnaireId: '789',\r\n            isPublished: false,\r\n            answersCount: 50,\r\n            creationDate: '2024-04-21'\r\n          },\r\n          {\r\n            questionnaireName: '问卷5',\r\n            questionnaireId: '789',\r\n            isPublished: false,\r\n            answersCount: 50,\r\n            creationDate: '2024-04-21'\r\n          },\r\n          {\r\n            questionnaireName: '问卷6',\r\n            questionnaireId: '789',\r\n            isPublished: false,\r\n            answersCount: 50,\r\n            creationDate: '2024-04-21'\r\n          },\r\n        ];\r\n\r\n        // 将获取的数据赋值给组件的数据属性\r\n        this.questionnaires = questionnaireDataFromBackend;\r\n      },\r\n      previousPage() {\r\n        console.log(this.currentPage);\r\n        if (this.currentPage > 1) {\r\n          this.currentPage-=1;\r\n          console.log(this.currentPage);\r\n        }\r\n        console.log(this.currentPage);\r\n      },\r\n      nextPage() {\r\n        if (this.currentPage < this.totalPages) {\r\n          this.currentPage+=1;\r\n          console.log(this.currentPage);\r\n        }\r\n        console.log(this.currentPage);\r\n      }\r\n    },\r\n  }\r\n\r\n  </script>\r\n  \r\n  <style>\r\n  #manage {\r\n    display: flex;\r\n  }\r\n\r\n  .qnentry{\r\n    display: flex;\r\n  }\r\n\r\n  .Instance {\r\n    margin-top: 20px;\r\n\r\n  }\r\n  \r\n  #sidebar {\r\n    width: 20%;\r\n    height: 730px;\r\n    background-color: #757070a0;\r\n    padding: 20px;\r\n    box-sizing: border-box;\r\n  }\r\n  \r\n  .button {\r\n    display: block;\r\n    width: 100%;\r\n    text-align: center;\r\n    padding: 10px;\r\n    margin-bottom: 10px;\r\n    border: none;\r\n    cursor: pointer;\r\n    outline: none;\r\n  }\r\n  \r\n  .button:hover {\r\n    border: 1px solid #000;\r\n  }\r\n  \r\n  #create-survey {\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  #bind-wechat {\r\n    background-color: #28a745;\r\n    color: #fff;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  #description {\r\n    margin-top: 250px;\r\n    background-color: white;\r\n    padding: 20px;\r\n  }\r\n  \r\n  #description-text {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .toolbar {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n\r\n    .sort-container{\r\n        margin-left: 520px;\r\n        margin-right: 20px;\r\n    