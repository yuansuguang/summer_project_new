emplate>\r\n    </el-dropdown>\r\n    <div class=\"auth1\" v-if=\"!isLogin\">\r\n      <el-button @click=\"gotologin\" size=\"default\" color=\"#626aef\" :dark=\"isDark\">登录</el-button>\r\n      <el-button @click=\"gotoregister\" size=\"default\" color=\"#626aef\" :dark=\"isDark\" plain>注册</el-button>\r\n    </div>\r\n    <div class=\"auth2\" v-if=\"isLogin\">\r\n      <el-button @click=\"gotologout\" size=\"default\" color=\"#626aef\" :dark=\"isDark\">登出</el-button> \r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport user from \"@/storage/user\";\r\nexport default {\r\n  name: 'normalPageHeader',\r\n  data() {\r\n    return {\r\n      userName: '',\r\n      activeIndex: '1',\r\n      isLogin: \"false\",\r\n    };\r\n    \r\n  },\r\n  created() {\r\n    const userInfo = user.getters.getUser(user.state());\r\n    console.log(userInfo);\r\n    console.log(this.isLogin);\r\n    if (userInfo) {\r\n      console.log('enter');\r\n      this.isLogin = true;\r\n      this.userName = userInfo.user.userName;\r\n    }\r\n  },\r\n  methods: {\r\n    handleSelect(key, keyPath) {\r\n      console.log(key, keyPath)\r\n    },\r\n    gobacktoHome: function () {\r\n      this.$router.push('/');\r\n    },\r\n    gotoaccountCenter(){\r\n      this.$router.push('/accountCenter');\r\n    },\r\n    gotologin() {\r\n      this.$router.push('/login');\r\n      console.log(\"login!\");\r\n    },\r\n    gotoregister() {\r\n      this.$router.push('/register');\r\n      console.log(\"register!\");\r\n    },\r\n    gotoqnManage(){\r\n      this.$router.push('/questionairemanage');\r\n      console.log(\"manage!\");\r\n    },\r\n    gotologout(){\r\n      // this.isLogin = false;\r\n      // this.userName = '\r\n      this.$axios({\r\n          method: 'get',\r\n          url: '/user/api/logout/',\r\n          // data: formData,\r\n        }).then(res => {\r\n          switch (res.data.status_code) {\r\n            case 200:\r\n              this.$store.dispatch('clear');\r\n              console.log('run')\r\n              location.reload();\r\n              break;\r\n            case 401:\r\n              // this.$message.error('not logged in');\r\n              // console.log(this.isLogin);\r\n              this.$store.dispatch('clear');\r\n              location.reload();\r\n              break;\r\n          }\r\n        }).catch(err => {\r\n          console.log(err);\r\n        });\r\n        this.$router.push('/mainPane');\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.header {\r\n  display: flex;\r\n  align-items: center;\r\n  /* justify-content: space-between; */\r\n  width: 100%;\r\n  padding: 10px;\r\n  margin-top: -8px;\r\n  background-color: white;\r\n  position: relative;\r\n  position: fixed;\r\n  z-index: 10; /* 确保头部在其他内容上方 */\r\n}\r\n\r\n.logo {\r\n  width: 50px;\r\n  height: 50px;\r\n  margin-right: 30px;\r\n}\r\n\r\n.buttons {\r\n  display: flex;\r\n}\r\n\r\n.buttons button {\r\n  margin-right: 10px;\r\n}\r\n\r\n\r\n.auth {\r\n  display: flex;\r\n  position: absolute;\r\n  top: 25%;\r\n  right: 5%;\r\n}\r\n\r\n.nav-button {\r\n  padding: 10px 20px;\r\n  font-size: 22px;\r\n  background-color: transparent;\r\n  border: none;\r\n  cursor: pointer;\r\n  color: #fff;\r\n  transition: color 0.3s;\r\n}\r\n\r\n.nav-button:hover {\r\n  color: blue;\r\n}\r\n</style>"],"mappings":";AA2CA,OAAOA,IAAG,MAAO,gBAAgB;AACjC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,GAAG;MAChBC,OAAO,EAAE;IACX,CAAC;EAEH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,QAAO,GAAIP,IAAI,CAACQ,OAAO,CAACC,OAAO,CAACT,IAAI,CAACU,KAAK,CAAC,CAAC,CAAC;IACnDC,OAAO,CAACC,GAAG,CAACL,QAAQ,CAAC;IACrBI,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,OAAO,CAAC;IACzB,IAAIE,QAAQ,EAAE;MACZI,OAAO,CAACC,GAAG,CAAC,OAAO,CAAC;MACpB,IAAI,CAACP,OAAM,GAAI,IAAI;MACnB,IAAI,CAACF,QAAO,GAAII,QAAQ,CAACP,IAAI,CAACG,QAAQ;IACxC;EACF,CAAC;EACDU,OAAO,EAAE;IACPC,YAAYA,CAACC,GAAG,EAAEC,OAAO,EAAE;MACzBL,OAAO,CAACC,GAAG,CAACG,GAAG,EAAEC,OAAO;IAC1B,CAAC;IACDC,YAAY,E