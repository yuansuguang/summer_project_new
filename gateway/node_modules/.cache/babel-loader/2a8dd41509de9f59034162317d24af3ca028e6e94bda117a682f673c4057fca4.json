\n    return {\r\n      questionnaireTitle: '',\r\n      questionType: '',\r\n      questionText: '',\r\n      questions: []\r\n    };\r\n  },\r\n  methods: {\r\n    addQuestion() {\r\n      if (this.questionText.trim() !== '') {\r\n        this.questions.push({\r\n          type: this.questionType,\r\n          text: this.questionText\r\n        });\r\n        // Clear input fields after adding question\r\n        this.questionType = '';\r\n        this.questionText = '';\r\n      }\r\n    },\r\n    removeQuestion(index) {\r\n      this.questions.splice(index, 1);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .question {\r\n    margin-bottom: 20px;\r\n    border: 1px solid #ccc;\r\n    padding: 10px;\r\n  }\r\n</style>\r\n"],"mappings":";AA4BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,kBAAkB,EAAE,EAAE;MACtBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACH,YAAY,CAACI,IAAI,CAAC,MAAM,EAAE,EAAE;QACnC,IAAI,CAACH,SAAS,CAACI,IAAI,CAAC;UAClBC,IAAI,EAAE,IAAI,CAACP,YAAY;UACvBQ,IAAI,EAAE,IAAI,CAACP;QACb,CAAC,CAAC;QACF;QACA,IAAI,CAACD,YAAW,GAAI,EAAE;QACtB,IAAI,CAACC,YAAW,GAAI,EAAE;MACxB;IACF,CAAC;IACDQ,cAAcA,CAACC,KAAK,EAAE;MACpB,IAAI,CAACR,SAAS,CAACS,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IACjC;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}