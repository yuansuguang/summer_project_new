ormData.append(\"description\", \"\");\n      console.log(userInfo.user.username);\n      var editUrlName = '';\n      switch (tag) {\n        case 'survey':\n          formData.append(\"type\", \"1\");\n          console(1);\n          editUrlName = 'questionaireEdit';\n          break;\n        case 'test':\n          console.log(\"enter here\");\n          formData.append(\"type\", \"3\");\n          editUrlName = 'questionaireEditTest';\n          break;\n        case 'vote':\n          formData.append(\"type\", \"2\");\n          editUrlName = 'questionaireEditVote';\n          break;\n        case 'checkin':\n          formData.append(\"type\", \"4\");\n          editUrlName = 'questionaireEdit';\n          break;\n        default:\n          formData.append(\"type\", \"5\");\n          editUrlName = 'EditHate';\n          break;\n      }\n\n      //console.log(formData);\n      for (let pair of formData.entries()) {\n        console.log(pair[0] + ', ' + pair[1]);\n      }\n      this.$axios({\n        method: 'post',\n        url: '/surveymanage/api/createsurvey',\n        data: formData\n      }).then(res => {\n        switch (res.data.status_code) {\n          case 1:\n            var surveyId = res.data.survey_id;\n            this.$router.push({\n              name: editUrlName,\n              params: {\n                qnid: surveyId\n              }\n            });\n            break;\n          case 2:\n            this.$message.warning(\"form invalid\");\n            break;\n          default:\n            this.$message.error(\"操作失败！\");\n            console.log(res.data.status_code);\n            break;\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    create(type) {\n      if (type == 'survey') {\n        this.$router.push('/questionaireTest'); //待修改\n      } else if (type == 'test') {\n        this.$router.push('/questionaireTest');\n      } else if (type == 'vote') {\n        this.$router.push('/questionaireVote');\n      } else if (type == 'checkin') {\n        this.$router.push('/questionaireTest'); //待修改\n      }\n    },\n    create1() {\n      this.dialogVisible = true;\n      this.quesType = 1;\n    }\n  }\n};","map":{"version":3,"names":["user","name","data","isHoverSurvey","isHoverTest","isHoverForm","isHoverVote","isHoverPunch","dialogVisible","quesType","surveyTitle","carouselCards","type","imageUrl","require","title","description","buttonType","methods","createSurvey","tag","formData","FormData","userInfo","getters","getUser","state","console","log","append","username","editUrlName","pair","entries","$axios","method","url","then","res","status_code","surveyId","survey_id","$router","push","params","qnid","$message","warning","error","catch","err","create","create1"],"sources":["E:\\QuestionairePlanet_Fronted\\mytest\\src\\views\\qn-createpane\\qnCreate.vue"],"sourcesContent":["<template v-slot:footer>\r\n  <div class=\"create-que\">    \r\n    <el-carousel trigger=\"hover\" height=\"710px\" indicator-position=\"outside\" type=\"card\" interval=\"4000\">\r\n      <el-carousel-item v-for=\"card in carouselCards\" :key=\"card.type\">\r\n        <el-card class=\"box-card\">\r\n          <div style=\"text-align: center;\">\r\n            <img class=\"image\" :src=\"card.imageUrl\" :alt=\"card.title\" style=\"margin-top: 20px\" />\r\n          </div>\r\n          <div>\r\n            <h3>{{ card.title }}</h3>\r\n            <p class=\"describe\" style=\"margin-top: -220px; margin-bottom: 40px;\">{{ card.description }}</p>\r\n            <el-button :type=\"card.buttonType\" @click=\"createSurvey(card.type)\" v-if=\"card.type !== 'more'\">立即创建</el-button>\r\n            <el-button v-if=\"card.type === 'more'\" style=\"visibility: hidden;\">占位按钮</el-button>\r\n          </div>\r\n        </el-card>\r\n      </el-carousel-item>\r\n    </el-carousel>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\n  import user from \"@/storage/user\";\r\n  \r\n  export default {\r\n   